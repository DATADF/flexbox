{
    "liveSassCompile.settings.formats": [{
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "/css/"
        },
        {
            "extensionName": ".min.css",
            "format": "compressed",
            "savePath": "/css/compressed/"
        },
        {
            "extensionName": ".css",
            "format": "nested",
            "savePath": "/css/"
        }
    ],
    "liveSassCompile.settings.excludeList": [
        "**/node_modules/**",
        ".vscode/**"
    ],
    "liveSassCompile.settings.generateMap": true
}